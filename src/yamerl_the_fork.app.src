%% -*-erlang-*-
%% vim:ft=erlang:

{application, yamerl_the_fork, [
    {description,
      "A stripped down fork of yamerl, a YAML parser in pure Erlang"
    },
    {vsn, {cmd, "awk '/^AC_INIT\\(/ { ver=$0; sub(/AC_INIT\\(\\[[^]]+\\], *\\[/, \"\", ver); sub(/\\].*/, \"\", ver); print ver; } { next; }' configure.ac"}},
    {modules, [
        %% DO NOT EDIT: Modules list is generated by Makefile.am.
      ]},
    {registered, [yamerl_the_fork_sup]},
    {applications, [
        kernel,
        stdlib
      ]},
    {licenses, ["2-clause BSD license"]},
    {links, [{"Github", "https://github.com/amorphid/yamerl_the_fork"}]},
    {maintainers, ["Michael Pope"]},
    {mod, {yamerl_the_fork_app, []}},
    {env, [
        %% Configuration default values.
        %% DO NOT EDIT: The final configuration is made in sys.config.
        {node_mods, []}
      ]}
  ]}.
